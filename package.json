{
  "name": "clubcard",
  "version": "1.0.2",
  "description": "Programmable ownership: allow holders of designated NFTs to sign transactions",
  "scripts": {
    "prepare": "yarn build",
    "prebuild": "rimraf build",
    "build": "yarn build:evm && yarn build:cjs && yarn build:esm",
    "build:evm": "yarn hardhat compile",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "deploy": "hardhat deploy --network",
    "verify": "hardhat verify --network",
    "check": "yarn check:prettier && yarn check:lint",
    "check:lint": "eslint sdk test --ext .ts",
    "check:prettier": "prettier '**/*.{sol,js,ts,tsx,json}' --ignore-path .gitignore --list-different",
    "fix": "yarn fix:prettier && yarn fix:lint",
    "fix:prettier": "prettier '**/*.{sol,js,ts,tsx,json}' --ignore-path .gitignore --write",
    "fix:lint": "yarn check:lint --fix",
    "test": "hardhat test",
    "test:watch": "mocha -w"
  },
  "files": [
    "build",
    "contracts",
    "LICENSE",
    "README.md"
  ],
  "main": "build/cjs/sdk/index.js",
  "typings": "build/cjs/sdk/index.d.ts",
  "module": "build/esm/sdk/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gnosis/clubcard.git"
  },
  "author": "Gnosis Guild",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/gnosis/clubcard/issues"
  },
  "homepage": "https://github.com/gnosis/clubcard#readme",
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.4",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.5",
    "hardhat-deploy": "^0.11.22",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^2.8.3",
    "prettier-plugin-solidity": "^1.1.1",
    "rimraf": "^4.1.2",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@gnosis.pm/safe-contracts": "^1.3.0",
    "@gnosis.pm/zodiac": "^3.0.0",
    "@openzeppelin/contracts": "^4.8.1",
    "@safe-global/safe-core-sdk": "^3.3.1",
    "@types/chai": "^4.3.4",
    "@types/node": "^18.11.18"
  }
}
